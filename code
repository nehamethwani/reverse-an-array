#include <iostream>
#include <math.h>
#include <limits.h>
using namespace std;
int main() {
	int n;
	cout<<"enter length of array: ";
	cin >> n;
	int arr[n];
	int start =0;
	int end = n-1;
	for(int i=0; i<n; i++){
	    cin >> arr[i];
	}
	while(start< end){
	    swap(arr[start], arr[end]);
	    start++;
	    end--;
	}
	for(int i=0; i<n; i++){
	    cout<< arr[i]<< " ";
	}
	
	
	
	
	return 0;
}
